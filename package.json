{
  "name": "zasada",
  "version": "0.0.27",
  "description": "UI widget factory on ES classes with DI",
  "keywords": [
    "javascript",
    "typescript",
    "factory",
    "library",
    "widget",
    "es2015",
    "es6",
    "dependency injection",
    "js",
    "di",
    "ui",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kraut-dps/zasada.git"
  },
  "homepage": "https://github.com/kraut-dps/zasada",
  "license": "MIT",
  "scripts": {
    "ts-types": "lerna run ts-types",
    "unit": "karma start karma.conf.js --single-run",
    "coverage": "karma start karma.conf.js --single-run --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-function-bind": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-syntax-async-generators": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.7.5",
    "babel-loader": "^8.0.6",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^2.9.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-virtualbox-ie11-launcher": "^1.2.0",
    "karma-webpack": "^3.0.5",
    "lerna": "^3.22.1",
    "serialize-javascript": ">=3.1.0",
    "typescript": "^4.0.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "^5.1.1"
  },
  "comments": {
    "devDependencies": {
      "@babel/plugin-proposal-function-bind": "::this.method syntax",
      "@babel/plugin-proposal-object-rest-spread": "...{} syntax",
      "@babel/plugin-syntax-async-generators": "sync await tests",
      "@babel/plugin-syntax-dynamic-import": "import()",
      "@babel/plugin-transform-runtime": "reuse babel helpers",
      "jasmine-core": "tests, old version for test with polyfills",
      "karma": "tests",
      "uglifyjs-webpack-plugin": "minify js",
      "istanbul-instrumenter-loader": "coverage",
      "webpack-merge": "merge two webpack configs"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@zasada/boilerplate": "file:packages/boilerplate",
    "@zasada/bootstrap": "file:packages/bootstrap",
    "@zasada/core": "file:packages/core",
    "@zasada/examples": "file:packages/examples",
    "@zasada/frame": "file:packages/frame",
    "@zasada/log": "file:packages/log",
    "@zasada/test": "file:packages/test",
    "@zasada/widget": "file:packages/widget"
  }
}
